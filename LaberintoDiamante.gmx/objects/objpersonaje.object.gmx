    {x = x - velocidad;}
     }
     
if (keyboard_check(vk_right))
    {
    if(!place_meeting(x+velocidad,y,objwall))&amp;&amp;(!place_meeting(x+velocidad,y,objcandadopadre))
    {x = x + velocidad;}
    }
    
